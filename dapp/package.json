{
  "name": "nftyfinance-dapp",
  "version": "0.0.1",
  "scripts": {
    "prepare:local": "cp local.graphclientrc.yml .graphclientrc.yml",
    "prepare:prod": "cp prod.graphclientrc.yml .graphclientrc.yml",
    "build:graphclient": "graphclient build && rm -f .graphclient/package.json",
    "start": "yarn prepare:local && yarn build:graphclient && vite",
    "build:local": "yarn prepare:local && yarn build:graphclient && tsc && vite build",
    "build:prod": "yarn prepare:prod && yarn build:graphclient && tsc && vite build",
    "preview": "vite preview",
    "format": "rome format ./src",
    "format:write": "rome format ./src --write",
    "lint": "rome check ./src "
  },
  "dependencies": {
    "@graphprotocol/client-urql": "^1.0.18",
    "buffer": "^6.0.3",
    "connectkit": "^1.2.3",
    "dayjs": "^1.11.7",
    "ethers": "^5.7.2",
    "graphql": "^16.6.0",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-router-dom": "^6.10.0",
    "react-slider": "^2.0.4",
    "urql": "^4.0.1",
    "util": "^0.12.5",
    "wagmi": "^0.12.12"
  },
  "devDependencies": {
    "@graphprotocol/client-cli": "^2.2.22",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-slider": "^1.3.1",
    "@vitejs/plugin-react": "^4.0.0",
    "rome": "^12.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.3"
  },
  "resolutions": {
    "ethereumjs-abi": "https://registry.npmjs.org/ethereumjs-abi/-/ethereumjs-abi-0.6.8.tgz"
  }
}
